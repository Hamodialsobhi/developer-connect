{
  "name": "devconnector",
  "version": "1.0.0",
  "description": "Socal network for developers",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "debug": "nodemon --inspect-brk server.js",
    "client": "npm start --prefix client",
    "dev": "export NODE_ENV=development && concurrently \"npm run server\" \"npm run client\"",
    "prod": "export NODE_ENV=production && concurrently \"npm run server\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "MERN",
    "React.js",
    "Node.js",
    "Express.js",
    "MongoDB"
  ],
  "author": "Chisimdi Damian Ezeanieto",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^13.7.0",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}
